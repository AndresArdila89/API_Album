<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="API_Album.details">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="title"></Label>
            <Label x:Name="id"></Label>
            <Label x:Name="albumId"></Label>
            <Image x:Name="urlImage"
                
                                        
                                        HeightRequest="150"
                                        WidthRequest="150"
                                        HorizontalOptions="Center"></Image>
            <Label x:Name="url"></Label>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>